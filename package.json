{
  "name": "web-node",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "workspaces": [
    "projects/*",
    "packages/typescript/*"
  ],
  "scripts": {
    "clean": "yarn workspaces foreach --jobs unlimited -ptvi run clean",
    "build": "yarn workspaces foreach --jobs unlimited -ptvi run build",
    "rebuild": "yarn run clean && yarn run build",
    "newbie:cicd": "yarn set version 3.3.1 && yarn install",
    "build:server": "yarn workspaces web-node build",
    "dev:server": "yarn workspace web-node dev",
    "watch": "yarn workspaces foreach --include '@web{-node}/*' --jobs unlimited -pvi run watch"
  },
  "dependencies": {
    "tar": "6.1.15",
    "ws": "8.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.2",
    "@types/copyfiles": "2",
    "@types/eslint": "8",
    "@types/node": "18.15.11",
    "@types/nodemon": "1.19.2",
    "@types/prettier-package-json": "2",
    "@types/shelljs": "0.8.11",
    "@types/tar": "6.1.5",
    "@types/ws": "8",
    "@types/yargs": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "ansi-styles": "6.2.1",
    "chalk": "4.1.2",
    "chokidar": "3.5.3",
    "compressing": "1.9.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "node-gyp": "9.0.0",
    "nodemon": "2.0.22",
    "organize-imports-cli": "0.10.0",
    "prettier": "2.8.6",
    "prettier-package-json": "2.8.0",
    "redis-cli": "2.1.2",
    "shelljs": "0.8.5",
    "timeout-cli": "0.3.2",
    "tree-kill": "1.2.2",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "yargs": "17.7.1"
  },
  "packageManager": "yarn@3.3.1"
}
